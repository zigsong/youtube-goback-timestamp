{
  "compilerOptions": {
   "target": "ES6",
   "module": "ES2015",
   "lib": ["DOM", "DOM.Iterable","ESNext"],
   "allowJs": true,
   "outDir": "./scripts", 
   "rootDir": "./src",
   "strict": true,
   "noImplicitAny": true, 
   "strictNullChecks": true,
   "strictFunctionTypes": true, 
   "strictPropertyInitialization": true,
   "noImplicitThis": true,
   "alwaysStrict": true,
   "noUnusedLocals": true,
   "noUnusedParameters": true,
   "noImplicitReturns": true,
   "noFallthroughCasesInSwitch": true, 
  }
 }